<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="capstone.project.SpotMate.mapper.LoginMapper">

    <select id="findUserByEmail">
        SELECT * FROM user where email = #{email} and state = #{state}
    </select>


</mapper>
